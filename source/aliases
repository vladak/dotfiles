alias aliases="alias | sed 's/=.*//'"
alias functions="declare -f | grep '^[a-z].* ()' | sed 's/{$//'"
alias paths='echo -e ${PATH//:/\\n}'

alias week="date +%V"

# some more ls aliases
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'

# OpenStreetMap
alias josm='javaws -Xmx2048m http://josm.openstreetmap.de/download/josm.jnlp'
